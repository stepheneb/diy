<h1>Listing probe_types</h1>

<table>
  <tr>
  </tr>
  
<% for probe_type in @probe_types %>
  <tr>
    <td><%= link_to 'Show', probe_type_path(probe_type) %></td>
    <td><%= link_to 'Edit', edit_probe_type_path(probe_type) %></td>
    <td><%= link_to 'Destroy', probe_type_path(probe_type), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

